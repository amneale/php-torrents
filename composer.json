{
    "name": "amneale/php-torrents",
    "description": "PHP library for manipulating torrent data",
    "type": "library",
    "authors": [
        {
            "name": "Adam Neale",
            "email": "adam@adam-neale.co.uk"
        }
    ],
    "require": {
        "php": "^8.3",
        "league/uri": "^7.5",
        "league/uri-components": "^7.5",
        "arokettu/bencode": "^4.3"
    },
    "require-dev": {
        "adlawson/vfs": "^0.12.1",
        "friendsofphp/php-cs-fixer": "^3.75",
        "phpspec/phpspec": "^7.5",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Amneale\\Torrent\\": "src/"
        }
    }
}
